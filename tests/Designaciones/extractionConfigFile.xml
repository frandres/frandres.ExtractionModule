<Configuration>
	<UnitRegExp>^(.+?)$</UnitRegExp>
	<MinimumHitRatio>0</MinimumHitRatio>
	<DocumentsFilePath>/home/frandres/Eclipse/workspace/ExtractionModule/tests/Designaciones/testSet/</DocumentsFilePath>
	<FieldDescriptors>
		<FieldDescriptor>
			<FieldName>Profesor.Nombre</FieldName>
			<SpecificRegExps>
				<SpecificRegExp>Prof.*?(\w+[a-zA-Z \.]*).*</SpecificRegExp>
			</SpecificRegExps>
			<Weight>4</Weight>
			<isDate>0</isDate>
		</FieldDescriptor>

		<FieldDescriptor>
			<FieldName>EsAsignado.fechaAsignacion</FieldName>
			<SpecificRegExps>
				<SpecificRegExp>a partir del.*?([0-9-/.]*?)</SpecificRegExp>
				<SpecificRegExp>.*([0-9-/\.]*?)</SpecificRegExp>
			</SpecificRegExps>
			<Weight>5</Weight>
			<isDate>1</isDate>
		</FieldDescriptor>		
		
		<FieldDescriptor>
			<FieldName>EsAsignado.fechaFinal</FieldName>
			<SpecificRegExps>
				<SpecificRegExp>hasta el.*?([0-9-/.]*?)</SpecificRegExp>
				<SpecificRegExp>.*([0-9-/\.]*?)</SpecificRegExp>
			</SpecificRegExps>
			<Weight>5</Weight>
			<isDate>1</isDate>
		</FieldDescriptor>		
		
		<FieldDescriptor>
			<FieldName>EsAsignado.motivo</FieldName>
			<SpecificRegExps>
				<SpecificRegExp>(por motivo .*?)$</SpecificRegExp>
				<SpecificRegExp>(por .*?)$</SpecificRegExp>
				<SpecificRegExp>(en sustituci\u00f3n.*?)$</SpecificRegExp>
			</SpecificRegExps>
			<Weight>5</Weight>
			<isDate>1</isDate>
		</FieldDescriptor>		
		
		<FieldDescriptor>
			<FieldName>EsAsignado.calificacion</FieldName>
			<SpecificRegExps>
				<SpecificRegExp>Jefe(.*?)</SpecificRegExp>
			</SpecificRegExps>
			<Weight>5</Weight>
			<isDate>0</isDate>
		</FieldDescriptor>	
	</FieldDescriptors>

</Configuration>